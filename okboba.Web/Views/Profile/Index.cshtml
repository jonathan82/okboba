@model okboba.Web.Models.ProfileViewModel

@{
    ViewBag.Title = "Profile";
    ViewBag.Section = "Profile";
}

@Html.Action("ProfileHeader", new {profileId = Model.ProfileId, isMe = Model.IsMe, section = "Profile" })

@Html.Action("ProfileDetailAndText", new { profileId = Model.ProfileId, isMe = Model.IsMe })

@Html.Partial("~/Views/Photo/_PhotoUpload.cshtml")

@section Scripts {

    <script>
        $('.js-editinplace-editicon').editinplace();        
        $('[data-toggle="photoupload"]').photoUpload();
    </script>
}